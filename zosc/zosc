#! /usr/bin/python
import cout
import cmd
import osc
#======================================================================

obs = osc.Obs()
#isc = obs.Isc()

print obs
for prj in obs.ls( "^zypp:" ):
  if prj.name == 'zypp:zc10': continue
  if prj.name != 'zypp:SLE-11-SP4-Branch': continue
  print "  %s" % prj
  for pkg in prj.ls( 'libzypp$' ):
    print "    %s" % pkg
    for file in pkg.ls():
      print "      %s" % file


#======================================================================
